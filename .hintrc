{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "typescript-config/consistent-casing": "off",
    "button-type": "off",
    "typescript-config/target": "off",
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-font-feature-settings",
          "scroll-behavior",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 106",
    "not safari <= 16",
    "not and_ff <= 105"
  ]
}